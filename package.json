    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lcov": "^1.16.0",
    "lint-staged": "^15.0.2",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "^3.6.2",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.0.0"

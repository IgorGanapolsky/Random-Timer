    "test:coverage": "jest --coverage",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "autofix": "npm run fmt:fix && npm run lint:fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "typecheck": "tsc --noEmit",
